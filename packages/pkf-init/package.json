{
  "name": "@pantheon-tech/pkf-init",
  "version": "1.0.1",
  "description": "AI-assisted PKF initialization for existing projects",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "pkf-init": "./dist/cli.js"
  },
  "files": [
    "dist",
    "agents",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pantheon-tech/pkf.git",
    "directory": "packages/pkf-init"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "pkf",
    "documentation",
    "ai",
    "initialization"
  ],
  "author": "Pantheon Tech",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@pantheon-tech/pkf-core": "^0.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "glob": "^11.0.0",
    "inquirer": "^12.3.0",
    "js-yaml": "^4.1.0",
    "ora": "^8.1.1",
    "semver": "^7.7.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@types/semver": "^7.7.1",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.18.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.21.0",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
